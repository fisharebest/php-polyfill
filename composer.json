{
	"name": "fisharebest/php-polyfill",
	"type": "library",
	"description": "Polyfills for PHP 5.3 onwards",
	"keywords": [
		"php",
		"polyfill",
		"shim",
		"compat",
		"compatibility"
	],
	"homepage": "https://github.com/fisharebest/php-polyfill",
	"license": "MIT and GPL-3.0+",
	"authors": [
		{
			"name": "Greg Roach",
			"email": "fisharebest@gmail.com"
		}
	],
	"require": {
		"fisharebest/ext-calendar": "~2.4",
		"jakeasmith/http_build_url": "1.0.*",
		"symfony/polyfill-apcu": "1.8.*",
		"symfony/polyfill-php54": "1.8.*",
		"symfony/polyfill-php55": "1.8.*",
		"symfony/polyfill-php56": "1.8.*",
		"symfony/polyfill-php70": "1.8.*",
		"symfony/polyfill-iconv": "1.8.*",
		"symfony/polyfill-intl-grapheme": "1.8.*",
		"symfony/polyfill-intl-icu": "1.8.*",
		"symfony/polyfill-intl-normalizer": "1.8.*",
		"symfony/polyfill-mbstring": "1.8.*",
		"symfony/polyfill-util": "1.8.*",
		"symfony/polyfill-xml": "1.8.*"
	},
	"require-dev": {
		"phpunit/phpunit": "*",
		"satooshi/php-coveralls": "~1.0"
	},
	"autoload": {
		"psr-4": {
			"Fisharebest\\PhpPolyfill\\": "src/"
		},
		"files": [
			"src/bootstrap.php"
		]
	}
}
